<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.bank.immuniity</groupId>
  <artifactId>spring-blank-mvc</artifactId>
  <packaging>war</packaging>
  <version>1.0</version>
  <name>spring-blank-mvc Maven Webapp</name>
  <url>http://maven.apache.org</url>
  
  <properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<spring.version>4.1.1.RELEASE</spring.version>
		<spring.security.version>3.2.3.RELEASE</spring.security.version>
		<junit.version>4.11</junit.version>
		<mysql.version>5.1.19</mysql.version>
		<log4j.version>1.2.17</log4j.version>
		<commons.logging.version>1.1.1</commons.logging.version>
		<version.jackson.core>2.4.1</version.jackson.core>
	</properties>
	
  <dependencies>

		<dependency>
			<groupId>com.bank.immuniity</groupId>
			<artifactId>hibernate-blank-db</artifactId>
			<version>1.0</version>
		</dependency>

		<dependency>
